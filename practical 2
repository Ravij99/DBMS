CREATE TABLE CORDER(CORDER_NO INT PRIMARY KEY, CUSTOMER_NO REFERENCES CUSTOMER(CUSTOMER_NO),DATE_PLACED VARCHAR(20),DATE_DELIVERED VARCHAR (20))
