INSERT INTO DEPOT VALUES (5,'WALES','UK',5)
INSERT INTO DEPOT VALUES (6,'NORTH','KENYA',6)
INSERT INTO DEPOT VALUES (16,'SOUTH','UK',2)








CREATE TABLE CUSTOMER (CUSTOMER_NO INT PRIMARY KEY, NAME VARCHAR(15), ADDRESS VARCHAR(15), DEPOT_NO REFERENCES DEPOT (DEPOT_NO), CREDIT_LIMIT INT)
